@model IEnumerable<ApplicationVersionTracking.Models.Event>

@foreach (var evnt in Model)
{    
<tr class="d-flex">
    <td class="col-3">
        <font class="@(evnt.EnvironmentType.Description == "Development" ? "badge badge-warning" : evnt.EnvironmentType.Description == "Test" ? "badge badge-info" : "badge badge-success")">
            @evnt.EnvironmentType.Description
        </font>
    </td>
    <td class="col-3">
        @evnt.Version<br />
        <small><i>@evnt.msiFile</i></small>
    </td>
    <td class="col-3">
        <small>
            <a href="~/Servers/Details?id=@evnt.ServerID">@evnt.Server.Name</a>
        </small>
    </td>
    <td class="col-3">
        @evnt.EventDate.ToString("g")<br />
        <small><i>@evnt.Installer</i></small>
    </td>

</tr>
}
